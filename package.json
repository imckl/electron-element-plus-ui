{
  "name": "@imckl/electron-element-plus-ui",
  "version": "0.0.0",
  "description": "Electron + Element Plus 应用的 UI 组件库",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./renderer": {
      "types": "./dist/renderer/index.d.ts",
      "import": "./dist/renderer/index.js",
      "require": "./dist/renderer/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "vue": ">=3.3.0",
    "element-plus": ">=2.4.0"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "element-plus": "^2.13.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vue": "^3.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "imckl <imckl@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/imckl/electron-element-plus-ui"
  },
  "keywords": [
    "electron",
    "element-plus",
    "vue3",
    "layout",
    "ui-components"
  ]
}
